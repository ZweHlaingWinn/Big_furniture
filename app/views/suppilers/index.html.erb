<p id="notice"><%= notice %></p>

<h1>Suppilers</h1>

<table>
  <thead>
    <tr>
      <th>Suppiler name</th>
      <th>Phone</th>
      <th>Email</th>
      <th>Address</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @suppilers.each do |suppiler| %>
      <tr>
        <td><%= suppiler.suppiler_name %></td>
        <td><%= suppiler.phone %></td>
        <td><%= suppiler.email %></td>
        <td><%= suppiler.address %></td>
        <td><%= link_to 'Show', suppiler %></td>
        <td><%= link_to 'Edit', edit_suppiler_path(suppiler) %></td>
        <td><%= link_to 'Destroy', suppiler, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Suppiler', new_suppiler_path %>
